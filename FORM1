namespace LatadeTinta
{
    public partial class Form1 : Form
    {
        public Form1()
        {
            InitializeComponent();
        }
        private void button1_Click(object sender, EventArgs e)
        {
            if (radioButton1.Checked == true)
            {
                pictureBox8.BackColor = Color.Green;
            }
            if (radioButton2.Checked == true)
            {
                pictureBox8.BackColor = Color.OrangeRed;
            }
            if (radioButton3.Checked == true)
            {
                pictureBox8.BackColor = Color.SteelBlue;
            }
            if (radioButton4.Checked == true)
            {
                pictureBox8.BackColor = Color.Red;
            }
            if (radioButton5.Checked == true)
            {
                pictureBox8.BackColor = Color.Yellow;
            }
            if (radioButton6.Checked == true)
            {
                pictureBox8.BackColor = Color.Blue;
            }
            if (radioButton7.Checked == true)
            {
                pictureBox8.BackColor = Color.Violet;
            }
        }


    }
}
